{
  "name": "nuxt-apollo",
  "version": "1.0.0",
  "description": "Vue, Nuxt.js, GraphQL, Apollo Client starter kit.",
  "author": "Abdulhalim Kara <ahalimkara@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "backpack dev",
    "build": "nuxt build && backpack build",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "generate": "nuxt generate",
    "et:i": "node_modules/.bin/et -i assets/vendor/element/variables.scss",
    "et": "rm -rf assets/vendor/element/theme && node_modules/.bin/et -c assets/vendor/element/variables.scss -o assets/vendor/element/theme",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/apollo": "^3.0.1",
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.0",
    "apollo-link": "^1.0.7",
    "apollo-link-context": "^1.0.3",
    "apollo-link-http": "^1.3.2",
    "cookie": "^0.3.1",
    "element-ui": "^2.0.11",
    "express": "^4.16.2",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "js-cookie": "^2.2.0",
    "nuxt": "^1.0.0",
    "nuxt-routes": "^0.0.6",
    "vue-apollo": "^3.0.0-alpha.3",
    "vue-i18n": "^7.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "backpack-core": "^0.5.0",
    "cross-env": "^5.1.3",
    "element-theme": "^2.0.1",
    "element-theme-chalk": "^2.0.11",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6"
  },
  "license": "MIT",
  "keywords": [
    "Vue",
    "Nuxt.js",
    "GraphQL",
    "Apollo",
    "Element"
  ]
}
